{
  "name": "neo4j-ts-client",
  "version": "0.0.5",
  "description": "DB client for Neo4j written in Typescript and Javascript",
  "main": "dist/db-client.js",
  "scripts": {
    "cypher": "node -r ts-node/register/transpile-only src/db-client.ts",
    "db-funcs": "node -r ts-node/register/transpile-only src/db-funcs.ts",
    "db-solo": "node -r ts-node/register/transpile-only src/solograph/db-solograph.ts",
    "build": "tsc",
    "build-web": "esbuild src/db-client.ts --bundle --outfile=dist/db-client-web.js --platform=node --format=iife",
    "start": "node dist/db-client.js",
    "start-web": "http-server --cors -o db-client-neo4j-web.html",
    "pretest": "$npm_execpath run build",
    "test": "node dist/db-client.js -- ok",
    "pretest:write": "$npm_execpath run build",
    "test:write": "node dist/db-funcs.js -- runTestFuncs ok=1 echo=1 nodeCount=1 initValueConstraint=1 setValue=1 getValue=1 getAllValues=1 sample1=1 testNestedParms=1 testImplictInTransactions=1"
  },
  "keywords": [
    "neo4j",
    "db",
    "typescript",
    "javascript",
    "graph"
  ],
  "author": "Peter Dimitrios",
  "license": "MIT",
  "dependencies": {
    "neo4j-driver": "^5.28.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "esbuild": "^0.25.0"
  }
}
